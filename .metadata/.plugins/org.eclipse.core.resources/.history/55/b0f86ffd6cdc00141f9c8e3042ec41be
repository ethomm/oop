package salgsoversikt;

import java.util.ArrayList;

public class SalgsLinje {

//	private ArrayList<Produkt> salgslinjeListe = new ArrayListe<>; 
	private int spesAntall;
	private double rabatt;
	
	public SalgsLinje(int spesAntall, double rabatt) {
		super();
		this.spesAntall = spesAntall;
		this.rabatt = rabatt;
	}

	public int getSpesAntall() {
		return spesAntall;
	}

	public void setSpesAntall(int spesAntall) {
		this.spesAntall = spesAntall;
	}

	public double getRabatt() {
		return rabatt;
	}

	public void setRabatt(double rabatt) {
		this.rabatt = rabatt;
	}
	
	
}
